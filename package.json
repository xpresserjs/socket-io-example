{
  "name": "@xpresser/socket-io-example",
  "version": "1.0.0",
  "description": "XpresserJs SocketIO example",
  "main": "app.ts",
  "repository": "https://github.com/xpresserjs/new-app-lite-ts",
  "author": "xpresserjs",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "npm run build && node build/app.js",
